<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE decorators>
<decorators defaultdir="/template">
	<excludes>
		<pattern>/error.fhtml</pattern>
	</excludes>
	<decorator name="m-main" page="m/main.fhtml">
		<pattern>/m/*</pattern>
	</decorator>
	<decorator name="uc-main" page="u/uc-main.fhtml">
		<pattern>/uc/*</pattern>
	</decorator>
	<decorator name="u-main" page="u/u-main.fhtml">
		<pattern>/u/*</pattern>
	</decorator>
	<decorator name="w-main" page="w/main.fhtml">
		<pattern>/w/*</pattern>
	</decorator>
	<decorator name="simple" page="simple/main.fhtml">
		<pattern>/*.fhtml</pattern>
	</decorator>
</decorators>