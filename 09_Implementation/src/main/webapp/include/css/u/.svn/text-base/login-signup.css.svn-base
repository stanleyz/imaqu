}

.grey-border-b {
	border-bottom: 1px solid #a8a8a8;
}

.hint-sign {
	padding-left: 5px;
}

.hint-sign img {
	padding-right: 5px;
}

#hint-info {
	color: red;
	margin: 10px 0 10px 15px;
}

#hint-info li {
	padding: 3px 0;
}