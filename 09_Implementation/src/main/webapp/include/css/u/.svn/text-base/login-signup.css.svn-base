	padding: 10px;
	width: 590px;
}

.innerframe {
	background-color: #fff;
	margin: auto;
	padding: 10px 0 10px 10px;
	width: 580px;
}

.frame-title {
	letter-spacing: 10px;
	padding-bottom: 20px;
}

.b-submit {
	margin-right: 10px;
	width: 98px;
